# =============================================================
# TESTNET - ACCOUNT CREDENTIALS
# =============================================================

# --- TESTNET / MAIN ---
# TESTNET_CIRCULAR_MAIN_PUBLIC_ACCOUNT_PUBKEY="<YOUR_TESTNET_MAIN_ACCOUNT_ADDRESS>"
# TESTNET_CIRCULAR_MAIN_PUBLIC_ACCOUNT_PVTKEY="<YOUR_TESTNET_MAIN_ACCOUNT_PRIVATE_KEY>"

# # --- TESTNET / SECONDARY ---
# TESTNET_CIRCULAR_SECONDARY_PUBLIC_ACCOUNT_PUBKEY="<YOUR_TESTNET_SECONDARY_ACCOUNT_ADDRESS>"
# TESTNET_CIRCULAR_SECONDARY_PUBLIC_ACCOUNT_PVTKEY="<YOUR_TESTNET_SECONDARY_ACCOUNT_PRIVATE_KEY>"

# # --- TESTNET / DOCUMENT ---
# TESTNET_CIRCULAR_DOCUMENT_PUBLIC_ACCOUNT_PUBKEY="<YOUR_TESTNET_DOCUMENT_ACCOUNT_ADDRESS>"
# TESTNET_CIRCULAR_DOCUMENT_PUBLIC_ACCOUNT_PVTKEY="<YOUR_TESTNET_DOCUMENT_ACCOUNT_PRIVATE_KEY>"

# --- TESTNET / SANDBOX ---
TESTNET_CIRCULAR_SANDBOX_ACCOUNT_PUBKEY="<YOUR_TESTNET_SANDBOX_ACCOUNT_ADDRESS>"
TESTNET_CIRCULAR_SANDBOX_ACCOUNT_PVTKEY="<YOUR_TESTNET_SANDBOX_ACCOUNT_PRIVATE_KEY>"


